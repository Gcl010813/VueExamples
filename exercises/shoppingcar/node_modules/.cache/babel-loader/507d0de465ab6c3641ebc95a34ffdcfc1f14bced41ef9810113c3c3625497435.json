{"ast":null,"code":"import Header from '@/components/Header.vue';\nimport Footer from '@/components/Footer.vue';\nimport Item from '@/components/Item.vue';\nimport EventBus from './components/EventBus';\nexport default {\n  data() {\n    return {\n      itemLists: [{\n        id: 1,\n        status: true,\n        src: '...',\n        info: '商品1',\n        price: 1.99,\n        count: 1\n      }, {\n        id: 2,\n        status: false,\n        src: '...',\n        info: '商品2',\n        price: 1,\n        count: 1\n      }, {\n        id: 3,\n        status: true,\n        src: '...',\n        info: '商品3',\n        price: 1.89,\n        count: 1\n      }],\n      itemsCount: 0,\n      itemsCost: 0\n    };\n  },\n  components: {\n    Header,\n    Item,\n    Footer\n  },\n  methods: {\n    changeItemStatusOrCount(id, status, count) {\n      this.itemLists[id - 1].status = status;\n      this.itemLists[id - 1].count = count;\n    },\n    AppToFooter() {\n      this.itemsCount = this.itemLists.filter(item => item.status).reduce((itemsCount, item) => {\n        return itemsCount + item.count;\n      }, 0);\n      this.itemsCost = this.itemLists.filter(item => item.status).reduce((itemsCost, Item) => {\n        return itemsCost + Item.count * Item.price;\n      }, 0).toFixed(2);\n    }\n  },\n  updated() {\n    this.AppToFooter();\n    EventBus.$emit(\"AppToFooter\", this.itemsCount, this.itemsCost);\n  },\n  created() {\n    this.AppToFooter();\n  }\n};","map":{"version":3,"names":["Header","Footer","Item","EventBus","data","itemLists","id","status","src","info","price","count","itemsCount","itemsCost","components","methods","changeItemStatusOrCount","AppToFooter","filter","item","reduce","toFixed","updated","$emit","created"],"sources":["src/App.vue"],"sourcesContent":["<template>\n    <div id=\"app\">\n        <Header></Header>\n        <Item v-for=\"(item, index) in itemLists\" :key=\"index\" :idRef=\"item.id\" :statusRef=\"item.status\" :srcRef=item.src\n            :infoRef=item.info :priceRef=item.price :countRef=\"item.count\"\n            @changeItemStatusOrCount=\"changeItemStatusOrCount\">\n        </Item>\n        <Footer :itemsCountRef=\"itemsCount\" :itemsCostRef=\"itemsCost\"></Footer>\n    </div>\n</template>\n\n<script>\nimport Header from '@/components/Header.vue';\nimport Footer from '@/components/Footer.vue';\nimport Item from '@/components/Item.vue';\nimport EventBus from './components/EventBus';\n\nexport default {\n    data() {\n        return {\n            itemLists: [\n                { id: 1, status: true, src: '...', info: '商品1', price: 1.99, count: 1 },\n                { id: 2, status: false, src: '...', info: '商品2', price: 1, count: 1 },\n                { id: 3, status: true, src: '...', info: '商品3', price: 1.89, count: 1 },\n            ],\n            itemsCount: 0,\n            itemsCost: 0,\n        }\n    },\n    components: {\n        Header,\n        Item,\n        Footer,\n    },\n    methods: {\n        changeItemStatusOrCount(id, status, count) {\n            this.itemLists[id - 1].status = status\n            this.itemLists[id - 1].count = count\n        },\n        AppToFooter() {\n            this.itemsCount = this.itemLists.filter(item => item.status).reduce((itemsCount, item) => {\n                return itemsCount + item.count\n            }, 0)\n            this.itemsCost = this.itemLists.filter(item => item.status).reduce((itemsCost, Item) => {\n                return itemsCost + Item.count * Item.price\n            }, 0).toFixed(2)\n        }\n    },\n    updated() {\n        this.AppToFooter()\n        EventBus.$emit(\"AppToFooter\", this.itemsCount, this.itemsCost)\n    },\n    created() {\n        this.AppToFooter()\n    }\n}\n</script>\n\n<style lang=\"less\" scoped></style>"],"mappings":"AAYA,OAAAA,MAAA;AACA,OAAAC,MAAA;AACA,OAAAC,IAAA;AACA,OAAAC,QAAA;AAEA;EACAC,KAAA;IACA;MACAC,SAAA,GACA;QAAAC,EAAA;QAAAC,MAAA;QAAAC,GAAA;QAAAC,IAAA;QAAAC,KAAA;QAAAC,KAAA;MAAA,GACA;QAAAL,EAAA;QAA