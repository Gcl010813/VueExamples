lt: true,\r\n            type: Boolean,\r\n        },\r\n        srcRef: {\r\n            default: '...',\r\n            type: String,\r\n        },\r\n        infoRef: {\r\n            default: '商品x',\r\n            type: String\r\n        },\r\n        priceRef: {\r\n            default: 99999,\r\n            type: Number,\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            itemId: this.idRef,\r\n            itemStatus: this.statusRef,\r\n            itemSrc: this.srcRef,\r\n            itemInfo: this.infoRef,\r\n            //保留两位小数\r\n            itemPrice: this.priceRef.toFixed(2),\r\n        }\r\n    },\r\n    methods: {\r\n        changeItemStatus(e) {\r\n            this.$emit('ItemToAppStatus', { id: this.itemId, status: e.target.checked })\r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#item {\r\n    display: flex;\r\n    height: 100px;\r\n    border-radius: 10px;\r\n    background-color: rgb(187, 163, 163);\r\n    overflow: hidden;\r\n    margin: 5px 0;\r\n\r\n    .select {\r\n        margin: 0 3px;\r\n    }\r\n\r\n    img {\r\n        flex: 1;\r\n        height: 100%;\r\n        border-radius: 10px;\r\n        background-color: pink;\r\n    }\r\n\r\n    .info {\r\n        flex: 3;\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: space-between;\r\n        height: 100%;\r\n        border-radius: 10px;\r\n        padding: 5px;\r\n\r\n        .text {\r\n            height: 50px;\r\n            font-weight: 600;\r\n            overflow: hidden;\r\n        }\r\n\r\n        .footer {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            padding: 0 10px;\r\n\r\n            .price {\r\n                font-weight: 800;\r\n                color: red;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>"],"mappings":"AAiBA;EACA;EACAA,KAAA;IACAC,KAAA;MACAC,OAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,QAAA;IACA;IACAC,SAAA;MACAJ,OAAA;MACAC,IAAA,