{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { Toast } from 'vant';\nimport { getArticlesAPI } from '@/api/articlesAPI.js';\nimport ArticlesInfoVue from '@/components/article/ArticleInfo.vue';\nexport default {\n  name: 'myHome',\n  data() {\n    return {\n      page: 1,\n      limit: 10,\n      articleList: [],\n      isLoading: false\n    };\n  },\n  components: {\n    ArticlesInfoVue\n  },\n  methods: {\n    async initArticleList() {\n      const {\n        data: res\n      } = await getArticlesAPI(this.page, this.limit);\n      this.articleList.push(res);\n    },\n    onRefresh() {\n      setTimeout(() => {\n        Toast('刷新成功');\n        this.isLoading = false;\n      }, 1000);\n    },\n    onLoad() {\n      // 异步更新数据\n      setTimeout(() => {\n        for (let i = 0; i < 10; i++) {\n          this.list.push(this.list.length + 1);\n        }\n\n        // 加载状态结束\n        this.loading = false;\n\n        // 数据全部加载完成\n        if (this.list.length >= 40) {\n          this.finished = true;\n        }\n      }, 1000);\n    }\n  },\n  created() {\n    this.initArticleList();\n  }\n};","map":{"version":3,"names":["Toast","getArticlesAPI","ArticlesInfoVue","name","data","page","limit","articleList","isLoading","components","methods","initArticleList","res","push","onRefresh","setTimeout","onLoad","i","list","length","loading","finished","created"],"sources":["src/views/home/Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <van-pull-refresh\r\n      v-model=\"isLoading\"\r\n      @refresh=\"onRefresh\"\r\n    ></van-pull-refresh>\r\n    <van-nav-bar title=\"小郭头条\" fixed placeholder />\r\n    <van-list\r\n      v-model=\"loading\"\r\n      :finished=\"finished\"\r\n      finished-text=\"没有更多了\"\r\n      @load=\"onLoad\"\r\n    >\r\n      <ArticlesInfoVue\r\n        v-for=\"(item, index) in articleList\"\r\n        :key=\"index\"\r\n        :title=\"item.title\"\r\n        :author=\"item.aut_name\"\r\n        :cmt-count=\"item.comm_count\"\r\n        :time=\"item.pubdate\"\r\n        :cover=\"item.cover\"\r\n      ></ArticlesInfoVue>\r\n    </van-list>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Toast } from 'vant'\r\nimport { getArticlesAPI } from '@/api/articlesAPI.js'\r\nimport ArticlesInfoVue from '@/components/article/ArticleInfo.vue'\r\nexport default {\r\n  name: 'myHome',\r\n  data () {\r\n    return {\r\n      page: 1,\r\n      limit: 10,\r\n      articleList: [],\r\n      isLoading: false\r\n    }\r\n  },\r\n  components: {\r\n    ArticlesInfoVue\r\n  },\r\n  methods: {\r\n    async initArticleList () {\r\n      const { data: res } = await getArticlesAPI(this.page, this.limit)\r\n      this.articleList.push(res)\r\n    },\r\n    onRefresh () {\r\n      setTimeout(() => {\r\n        Toast('刷新成功')\r\n        this.isLoading = false\r\n      }, 1000)\r\n    },\r\n    onLoad () {\r\n      // 异步更新数据\r\n      setTimeout(() => {\r\n        for (let i = 0; i < 10; i++) {\r\n          this.list.push(this.list.length + 1)\r\n        }\r\n\r\n        // 加载状态结束\r\n        this.loading = false\r\n\r\n        // 数据全部加载完成\r\n        if (this.list.length >= 40) {\r\n          this.finished = true\r\n        }\r\n      }, 1000)\r\n    }\r\n  },\r\n  created () {\r\n    this.initArticleList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.home {\r\n  /deep/ .van-nav-bar__content {\r\n    background-color: lightcoral;\r\n    border-radius: 20px;\r\n    .van-nav-bar__title {\r\n      font-weight: 800;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA2BA,SAAAA,KAAA;AACA,SAAAC,cAAA;AACA,OAAAC,eAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,WAAA;MACAC,SAAA;IACA;EACA;EACAC,UAAA;IACAP;EACA;EACAQ,OAAA;IACA,MAAAC,gBAAA;MACA;QAAAP,IAAA,EAAAQ;MAAA,UAAAX,cAAA,MAAAI,IAAA,OAAAC,KAAA;MACA,KAAAC,WAAA,CAAAM,IAAA,CAAAD,GAAA;IACA;IACAE,UAAA;MACAC,UAAA;QACAf,KAAA;QACA,KAAAQ,SAAA;MACA;IACA;IACAQ,OAAA;MACA;MACAD,UAAA;QACA,SAAAE,CAAA,MAAAA,CAAA,OAAAA,CAAA;UACA,