it('share', this.toBrotherInfo)\r\n        }\r\n    },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.Left {\r\n    width: 50%;\r\n    height: 200px;\r\n    float: left;\r\n    border: 1px solid #000;\r\n    border-width: 1px 0;\r\n    background-color: aqua;\r\n\r\n    h3 {\r\n        text-align: center;\r\n        color: blue;\r\n    }\r\n\r\n    span {\r\n        background-color: #fff;\r\n    }\r\n\r\n    // ::v-deep h5 {\r\n    /deep/ h5 {\r\n        color: greenyellow;\r\n    }\r\n}\r\n</style>"],"mappings":"AAeA,OAAAA,QAAA;AACA;EACA;EACA;EACAC,KAAA;EACAC,KAAA;IACA;MACAC,OAAA,OAAAC,UAAA;MACAC,QAAA,OAAAC,WAAA;MACAC,aAAA;IACA;EACA;EACAC,OAAA;IACA;IACAC,UAAA;MACA,KAAAN,OAAA;IACA;IACA;IACAO,WAAA;MACA,KAAAL,QAAA,CAAAM,IAAA;IACA;IACAC,UAAA;MACAZ,QAAA,CAAAa,KAAA,eAAAN,aAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}